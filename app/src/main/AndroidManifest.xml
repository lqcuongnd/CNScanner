            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.ReportDetailActivity" />
        <activity android:name=".Activities.ReportListActivity" />
        <activity android:name=".Activities.PersonalActivity" />
        <activity android:name=".Activities.SettingActivity" />
        <activity android:name=".Activities.ReportActivity" />
        <activity android:name=".Activities.LoginActivity" />
